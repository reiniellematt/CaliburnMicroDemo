<UserControl x:Class="WPFUI.Views.CView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WPFUI.Views"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800" Background="White">
    <StackPanel>
        <TextBlock HorizontalAlignment="Center" Text="Close App And Open New Window!" FontSize="24" Margin="0,48" />
        <Button x:Name="ShowNewView" HorizontalAlignment="Center" Content="Do It!" FontSize="24" Margin="0,48" Padding="8" />
    </StackPanel>
</UserControl>